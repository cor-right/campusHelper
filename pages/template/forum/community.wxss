.like-animate{ position: absolute; left: 50%; bottom: 17px; margin: -8px; color: red; backface-visibility:visible; -webkit-backface-visibility:visible; transform-origin:center center; -webkit-transform-origin:center center; animation:likeAnimate 0.5s 0s 1 ease-out normal ; -webkit-animation:likeAnimate 0.5s 0s 1 ease-out normal ; }
@-webkit-keyframes likeAnimate{ 0%{ opacity:1; -webkit-transform:translateY(0px); }
60%{ opacity:1; -webkit-transform:translateY(-10px); }
100%{ opacity:0; -webkit-transform:translateY(-15px); }
}@keyframes likeAnimate{ 0%{ opacity:1; -webkit-transform:translateY(0px); }
60%{ opacity:1; -webkit-transform:translateY(-10px); }
100%{ opacity:0; -webkit-transform:translateY(-15px); }}

