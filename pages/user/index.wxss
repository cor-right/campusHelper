body {
}

.weui-loadmore {
  width: 65%;
  margin: 50% auto;
  display: none;
  line-height: 1.6em;
  font-size: 13px;
  text-align: center;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.weui-loadmore_line {
  border-top: 1px solid #e5e5e5;
  margin-top: 50%;
}

.weui-loadmore__tips_in-line {
  padding: 0 0.55em;
  background-color: #f1f2fb;
  color: #999;
  margin-top: -27px;
}

.container {
  height: 100%;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  box-sizing: border-box;
}

.dbflex {
  display: -webkit-flex;
  display: flex;
}

.top_bor {
  position: fixed;
  top: 0;
  left: 0;
  height: 0;
  z-index: 9;
  border-bottom: 1px solid #e5e5e5;
  width: 100%;
}

.toast_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #fff;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.toast_screen_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
}

.toast_screen_view {
  bottom: 96px;
  font-size: 12pt;
  margin-bottom: 96px;
  width: 100%;
  height: -webkit-fit-content;
  height: fit-content;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-justify-content: center;
  justify-content: center;
}

.toast_screen_title_view {
  height: -webkit-fit-content;
  height: fit-content;
  width: -webkit-fit-content;
  width: fit-content;
  padding-bottom: 10px;
  padding-top: 10px;
  padding-left: 20px;
  padding-right: 20px;
  border: 1px #a9a9a9 solid;
  border-radius: 10px;
  color: #fff;
  background-color: #a9a9a9;
  text-align: center;
}

.nodata_more {
  font-size: 12px;
  color: #888;
  width: 100%;
  height: 43px;
  line-height: 43px;
  float: left;
  text-align: center;
}

.dn {
  display: none;
}

.db {
  display: block;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.clearfix::after {
  content: ".";
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}

.page_hd {
  width: 100%;
  line-height: 38px;
  font-size: 14px;
  text-align: center;
  float: left;
}

.page_hd_tips {
  display: inline-block;
  vertical-align: middle;
}

.page_hd-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

@-webkit-keyframes weuiLoading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes weuiLoading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

